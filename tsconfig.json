{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/binaryx": [
        "libs/binaryx/src"
      ],
      "@app/binaryx/*": [
        "libs/binaryx/src/*"
      ],
      "@app/blockchain-apps": [
        "libs/blockchain-apps/src"
      ],
      "@app/blockchain-apps/*": [
        "libs/blockchain-apps/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/lofty": [
        "libs/lofty/src"
      ],
      "@app/lofty/*": [
        "libs/lofty/src/*"
      ],
      "@app/realt": [
        "libs/realt/src"
      ],
      "@app/realt/*": [
        "libs/realt/src/*"
      ],
      "@app/sync": [
        "libs/sync/src"
      ],
      "@app/sync/*": [
        "libs/sync/src/*"
      ]
    }
  }
}