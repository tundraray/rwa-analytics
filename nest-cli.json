{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "binaryx": {
      "type": "library",
      "root": "libs/binaryx",
      "entryFile": "index",
      "sourceRoot": "libs/binaryx/src",
      "compilerOptions": {
        "tsConfigPath": "libs/binaryx/tsconfig.lib.json"
      }
    },
    "blockchain-apps": {
      "type": "library",
      "root": "libs/blockchain-apps",
      "entryFile": "index",
      "sourceRoot": "libs/blockchain-apps/src",
      "compilerOptions": {
        "tsConfigPath": "libs/blockchain-apps/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "lofty": {
      "type": "library",
      "root": "libs/lofty",
      "entryFile": "index",
      "sourceRoot": "libs/lofty/src",
      "compilerOptions": {
        "tsConfigPath": "libs/lofty/tsconfig.lib.json"
      }
    },
    "realt": {
      "type": "library",
      "root": "libs/realt",
      "entryFile": "index",
      "sourceRoot": "libs/realt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/realt/tsconfig.lib.json"
      }
    },
    "sync": {
      "type": "library",
      "root": "libs/sync",
      "entryFile": "index",
      "sourceRoot": "libs/sync/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sync/tsconfig.lib.json"
      }
    }
  }
}